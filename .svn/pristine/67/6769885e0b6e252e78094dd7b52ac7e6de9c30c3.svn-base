.btn-rigth {
	float: right;
}

.title-btn {
	margin-top: 41px;
}

header {
	margin: 0;
	padding: 0;
}

.sidebar {
	margin-top: 0px;
}

#logo {
	height: 150px;
	margin-left: 20px;
	width: 200px;
}

.fa {
	padding-left: 10px;
	padding-right: 10px;
}

i {
	margin-right: 5px;
}

h5 {
	font-weight: 700;
}

.panel-heading {
	text-decoration: underline;
	font-weight: bold;
	font-size: medium;
}

label {
	padding-top: 10px;
}

fieldset {
	padding-bottom: 10px;
}

.btn-circle {
	width: 30px;
	height: 30px;
	text-align: center;
	padding: 6px 0;
	font-size: 12px;
	line-height: 1.428571429;
	border-radius: 15px;
}

/*
 * Admin / non admin
 */
#container-application[data-admin='false'] .admin-block {
	display: none;
}

/*
 * Notifications
 */
#notification-area {
	position: fixed;
	right: 0;
	bottom: 0;
	width: auto;
	/*  IF IE10+ */
	pointer-events: none;
}

#notification-area>.notification {
	float: right;
	width: 440px;
	z-index: 100;
	margin-bottom: 12px;
	margin-right: 12px;
	pointer-events: auto;
}

#divNewJe {
	padding-bottom: 25px;
}

.modal-xl {
	width: 1080px !important;
}

.sidebar-search .dropdown-menu li {
	border-bottom: 0px;
}

.sidebar-search .autocomplete {
	right: 0;
	margin: 0 15px;
	top: 50px;
}

.sidebar ul li a.active {
	background-color: initial !important;
}

/**Source:
 * http://bootsnipp.com/snippets/featured/alerts-messages
 **/
hr.message-inner-separator {
	clear: both;
	margin-top: 10px;
	margin-bottom: 13px;
	border: 0;
	height: 1px;
	background-image: -webkit-linear-gradient(left, rgba(0, 0, 0, 0),
		rgba(0, 0, 0, 0.15), rgba(0, 0, 0, 0));
	background-image: -moz-linear-gradient(left, rgba(0, 0, 0, 0),
		rgba(0, 0, 0, 0.15), rgba(0, 0, 0, 0));
	background-image: -ms-linear-gradient(left, rgba(0, 0, 0, 0),
		rgba(0, 0, 0, 0.15), rgba(0, 0, 0, 0));
	background-image: -o-linear-gradient(left, rgba(0, 0, 0, 0),
		rgba(0, 0, 0, 0.15), rgba(0, 0, 0, 0));
}

.fa.fa-times {
	cursor: pointer;
}

#loading-bar-colored, #loading-bar-textual {
	position: fixed;
	height: 50px;
	width: 100%;
	bottom: 0;
	left: 0;
}

.panel {
	margin-bottom: 0 !important;
}

.sidebar {
	position: fixed;
	overflow-y: auto;
	height: 100%;
}

#loading-bar-colored {
	background-color: rgba(12, 12, 12, 0.35);
	z-index: 50000;
}

#loading-bar-textual {
	z-index: 50001;
	padding:17px 30px ;
	text-align: right;
}