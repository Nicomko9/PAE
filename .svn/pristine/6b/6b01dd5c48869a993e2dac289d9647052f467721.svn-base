<div id="inner-popup-company" class="modal fade" tabindex="-1"
	role="dialog" aria-hidden="true">
	<div class="container-fluid">
		<div class="row">
			<div class="modal-dialog modal-lg clearfix">
				<div class="modal-content">
					<div class="panel panel-default">
						<div class="panel-heading clearfix">
							<div class="col-md-10 col-md-offset-1 text-center">
								<span data-attribute="title"></span>
							</div>
							<div class="pull-right" data-dismiss="modal"
								data-target="#popup-new-company">
								<i class="fa fa-times"></i>
							</div>
						</div>
						<div class="panel-body">
							<div class="col-xs-12" id="EntPopupFormAlert"></div>
							<form role="form" id="popup-comp-form">
								<input type="hidden" name="pk" data-attribute="pk"> <input
									type="hidden" name="version" data-attribute="version">
								<div class="row">
									<div class="form-group col-lg-8 col-md-8">
										<label for="popup-name-entreprise"> Entreprise </label> <input
											type="text" class="form-control" id="popup-name-entreprise"
											data-required="true" name="name"
											placeholder="Entreprise S.A." data-attribute="name">
									</div>
								</div>
								<fieldset>
									<legend> Adresse de facturation </legend>
									<input type="hidden" name="pk-address"
										data-attribute="address.pk"> <input type="hidden"
										name="version-address" data-attribute="address.version">

									<div class="row">
										<div class="form-group col-lg-6 col-md-6">
											<label for="popup-street-entreprise"> Rue </label> <input
												type="text" class="form-control"
												data-attribute="address.street" name="street"
												id="popup-street-entreprise" data-required="true"
												placeholder="Rue">
										</div>
										<div class="form-group col-lg-2 col-md-2 ">
											<label for="popup-number-entreprise"> Numéro </label> <input
												type="number" class="form-control " name="number"
												data-attribute="address.number" id="popup-number-entreprise"
												data-required="true" placeholder="0" min="0">
										</div>
										<div class="form-group col-lg-2 col-md-2 ">
											<label for="popup-box-number-entreprise">Boite</label> <input
												type="text" class="form-control" name="box"
												data-attribute="address.box" id="popup-box-number-entreprise"
												placeholder="00">
										</div>
									</div>

									<div class="row">
										<div class="form-group col-lg-6 col-md-6">
											<label for="popup-city-name-entreprise">Ville</label> <input
												type="text" class="form-control"
												data-attribute="address.city" id="popup-city-name-entreprise"
												name="city" data-required="true" placeholder="Ville">
										</div>
										<div class="form-group col-lg-3 col-md-3 ">
											<label for="popup-postal-code-entreprise">Code postal</label>
											<input type="number" class="form-control "
														 data-required="true" id="popup-postal-code-entreprise"
														 data-attribute="address.zip" name="postal-code"
														 placeholder="1000" min="0">
										</div>
									</div>
								</fieldset>
							</form>
							<form role="form" id="popup-cont-comp-form"
								data-view="entreprise:registerContact" class="form">
								<legend>Contacts</legend>
								<div id="popup-contact-table">
									<div class="col-lg-12 col-md-12 ">
										<div class="table-responsive">
											<table class="table table-bordered table-condensed ">
												<thead class="thead-inverse">
													<tr>
														<th>Prenom</th>
														<th>Nom</th>
														<th>Téléphone</th>
														<th>E-mail</th>
														<th>Actif</th>
													</tr>
												</thead>
												<tbody>
													<tr id="popup-form-registration">
														<td class="col-sm-2 col-xs-2"><input
															class="form-control" type="hidden" name="companyPk" data-attribute="pk">
															<div class="form-group">
																<input class="form-control" data-required="true"
																			 type="text" placeholder="Prénom" name="firstname-contact">
															</div>
														</td>
														<td class="col-sm-3 col-xs-3" data-required="true">
															<div class="form-group">
																<input
																		class="form-control" type="text" placeholder="Nom"
																		name="lastname-contact" data-required="true">
															</div>
														</td>
														<td class="col-sm-3 col-xs-3">
															<div class="form-group">
																<input class="form-control" type="tel" name="tel-contact"
																		placeholder="0477/00.00.00">
															</div>
														</td>
														<td class="col-sm-3 col-xs-3">
															<div class="form-group">
																<input class="form-control" type="email" name="email-contact"
																		placeholder="nom.prenom@entreprise.be">
															</div>
														</td>
														<td class="text-center col-sm-1 col-xs-1">
															<button type="submit"
																class="fa fa-plus btn btn-success btn-circle">
															</button>
														</td>
													</tr>
												</tbody>
											</table>
										</div>
									</div>
								</div>
							</form>
							<div id="popup-participation-list">
								<div class="col-lg-12">
									<legend>Participations</legend>
									<table id="company-participations-table"
										class="table table-bordered table-condensed">
										<thead>
											<tr>
												<th>Année</th>
												<th>Etat</th>
												<th>Payée ?</th>
											</tr>
										</thead>
										<tbody>

										</tbody>
									</table>
								</div>
							</div>
						</div>
						<div class="panel-footer clearfix">
							<button class="btn btn-default pull-right" name="cancel-popup"
								data-dismiss="modal" data-target="#popup-new-company">Quitter</button>
							<a href="#" data-attribute="button-text"
								class="popup-confirm btn btn-primary pull-right"> </a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>