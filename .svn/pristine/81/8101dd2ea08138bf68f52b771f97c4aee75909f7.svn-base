<div name="page-title" class="row">
	<div class="col-lg-12 col-md-12">
		<h1 class="page-header">Journée Entreprise</h1>
	</div>
	<div class="page-container" id="container-JE">
		<div class="row">
			<div id="div-new-je" class="col-lg-10 col-lg-offset-1 admin-block">
				<div class="col-lg-offset-10 col-md-offset-10 col-lg-2 col-md-2">
					<a id="#" class="btn btn-info btn-rigth" data-view="je:createJe">
						Lancer nouvelle JE
					</a>
				</div>
			</div>
		</div>
	</div>
	<div id="invited-companies-table" class="col-lg-10 col-md-10 col-lg-offset-1 col-md-offset-1">
		<div class="panel panel-default">
			<div class="panel-heading">
				Journée entreprise : <span data-source="je.date"></span>
			</div>
			<div class="panel-body">
				<div class="row">
					<div class="col-lg-12 col-md-12">
							<h2>Entreprises invitées</h2>
					</div>
				</div>
				<div class="table-responsive">
					<table class="table table-bordered table-hover">
						<!-- table-striped table-bordered table-hover -->
						<div id="invited-entreprise-loading" style="display: none">
            				<i class="fa fa-spinner fa-spin"></i> Loading...
          				</div>
						<thead class="thead-inverse">
							<tr>
								<th>Entreprise</th>
								<th>Personnes de Contact</th>
								<th>Statut</th>
								<th>Action</th>
							</tr>
						</thead>
						<tbody>
							<!--<tr>
								<td class="col-lg-2 col-md-2">vide</td>
								<td class="col-lg-5 col-md-5">vide</td>
								<td class="col-lg-2 col-md-2">vide</td>
								<td class="col-lg-2 col-md-2 text-center">
									<button name="btn-cancel_participation" class="btn btn-danger">
										Annuler participation</button>
								</td>
							</tr>-->
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="page-container" style="display: none"
	id="container-invitation">
	<div class="row">
		<div
			class=" col-lg-10 col-md-10 col-lg-offset-1 col-md-offset-1 table-responsive">
			<table class="table table-bordered" id="tab-invite">
				<div id="selected-entreprise-loading" style="display: none">
            		<i class="fa fa-spinner fa-spin"></i> Loading...
          		</div>
				<thead class="thead-inverse">
					<tr>
						<th>Entreprise</th>
						<th>Date inscription</th>
						<th>Dernières participations</th>
						<th>Inviter</th>
					</tr>
				</thead>
				<tbody id="tbody-tab-invite">
					<tr class="invite">
						<td class="col-lg-2 col-md-2 td-invite">Testation</td>
						<td class="col-lg-4 col-md-4 td-invite">12/12/2012</td>
						<td class="col-lg-2 col-md-2 td-invite">2013</td>
						<td class="col-lg-4 col-md-4 text-center"><input
							type="checkbox" name="check-invite"></input></td>
					</tr>
					<tr class="invite">
						<td class="col-lg-2 col-md-2 td-invite">Testify</td>
						<td class="col-lg-4 col-md-4 td-invite">12/12/2013</td>
						<td class="col-lg-2 col-md-2 td-invite">2014</td>
						<td class="col-lg-4 col-md-4 text-center"><input
							type="checkbox" name="check-invite"></input></td>
					</tr>
					<tr class="invite">
						<td class="col-lg-2 col-md-2 td-invite">Test SA</td>
						<td class="col-lg-4 col-md-4 td-invite">12/12/2014</td>
						<td class="col-lg-2 col-md-2 td-invite">2015</td>
						<td class="col-lg-4 col-md-4 text-center"><input
							type="checkbox" name="check-invite"></input></td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-lg-offset-9 col-md-offset-9 col-lg-2 col-md-2 admin-block">
		<button id="btn-invite-entreprise" class="btn btn-info btn-rigth">Inviter
			entreprises</button>
	</div>
	<div class="col-lg-offset-10 col-md-offset-10 col-lg-2 col-md-2 admin-block">
		<button id="btn-invite-entreprises" class="btn btn-info btn-right"
			style="display:none">Inviter</button>
	</div>
</div>